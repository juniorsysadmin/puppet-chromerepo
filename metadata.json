{
  "name": "juniorsysadmin-chromerepo",
  "version": "0.0.3",
  "author": "Ben Spiccia",
  "summary": "Module for installing the official Google Chrome APT/YUM repository",
  "license": "Apache-2.0",
  "source": "git://github.com/juniorsysadmin/puppet-chromerepo.git",
  "project_page": "https://github.com/juniorsysadmin/puppet-chromerepo",
  "issues_url": "https://github.com/juniorsysadmin/puppet-chromerepo/issues",
  "tags": ["chrome", "google"],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian"
    },
    {
      "operatingsystem": "Fedora"
    },
    {
      "operatingsystem": "OpenSUSE"
    },
    {
      "operatingsystem": "Ubuntu"
    }
  ],
  "dependencies": [
    {
      "name":"puppetlabs/stdlib",
      "version_requirement":">= 4.1.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=3.6.0 < 5.0.0"
    }
  ]
}
